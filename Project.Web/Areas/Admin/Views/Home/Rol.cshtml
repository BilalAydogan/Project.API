<h2>Roller</h2>
<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#rolModal">
    Add New Role
</button>
<div id="divRoller">
</div>




<div class="modal fade" id="rolModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title fs-5" id="exampleModalLabel">Role Details</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <label for="inputRolAd" class="form-label">Role Name</label>
                <input type="text" id="inputRolAd" class="form-control">

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-primary" onclick="SaveRol()">Save</button>
            </div>
        </div>
    </div>
</div>
<script src="~/js/admin/rol.js" asp-append-version="true"></script>